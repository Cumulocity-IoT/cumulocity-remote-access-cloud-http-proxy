{
  "name": "cloud-http-proxy",
  "version": "1.0.3",
  "description": "",
  "main": "src/index.js",
  "private": "true",
  "scripts": {
    "build": "tsc",
    "build:release": "docker build -t cloud-http-proxy . && docker save cloud-http-proxy -o image.tar && zip ../cloud-http-proxy cumulocity.json image.tar",
    "dev": "tsx watch src/index.ts",
    "start": "ts-node src/index.ts",
    "start:docker": "docker run -it -p 3000:3000 --name http-proxy --env-file .env cloud-http-proxy",
    "docker:clean": "docker kill http-proxy || docker rm http-proxy"
  },
  "author": "Tristan Bastian",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@c8y/client": "^1019.2.9",
    "cookie-parse": "^0.4.0",
    "cron": "^3.1.6",
    "dotenv": "^16.4.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  }
}
